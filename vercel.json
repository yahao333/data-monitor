{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/webhook/manage/create",
      "methods": ["POST"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/manage/list/(.+)",
      "methods": ["GET"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/manage/delete/(.+)/(.+)",
      "methods": ["DELETE"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/(.+)",
      "methods": ["POST", "GET"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook",
      "methods": ["GET"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/share/(.+)",
      "status": 200,
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    {
      "src": "/project/(.+)",
      "status": 200,
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/share/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/project/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
