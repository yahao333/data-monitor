{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/webhook/manage/create",
      "methods": ["POST"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/manage/list/(.+)",
      "methods": ["GET"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/manage/delete/(.+)/(.+)",
      "methods": ["DELETE"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook/(.+)",
      "methods": ["POST", "GET"],
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/webhook",
      "methods": ["GET"],
      "dest": "/api/webhook.ts"
    }
  ]
}
